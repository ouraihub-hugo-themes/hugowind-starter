{
  "name": "hugowind-starter",
  "version": "1.0.0",
  "description": "ðŸš€ Quick start template for HugoWind theme - A modern Hugo theme inspired by AstroWind",
  "type": "module",
  "scripts": {
    "dev": "hugo server",
    "dev:debug": "hugo server --logLevel debug --disableFastRender",
    "pagefind": "pagefind --site public",
    "build": "hugo --minify --gc && pnpm run pagefind",
    "preview": "pnpm run build && hugo server --disableLiveReload"
  },
  "devDependencies": {
    "pagefind": "^1.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.25.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ouraihub-hugo-themes/hugowind-starter.git"
  },
  "keywords": [
    "hugo",
    "theme",
    "astrowind",
    "blog",
    "starter",
    "tailwindcss"
  ],
  "author": "OurAIHub",
  "license": "MIT"
}
